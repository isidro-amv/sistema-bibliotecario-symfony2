<h1>Tel√©fono</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Celular</th>
            <td>{{ entity.celular }}</td>
        </tr>
        <tr>
            <th>Telefono_1</th>
            <td>{{ entity.telefono1 }}</td>
        </tr>
        <tr>
            <th>Telefono_2</th>
            <td>{{ entity.telefono2 }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('phone') }}">
            Regresar a la lista
        </a>
    </li>
    <li>
        <a href="{{ path('phone_edit', { 'id': entity.id }) }}">
            Editar
        </a>
    </li>
    <li>
        <form action="{{ path('phone_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Eliminar</button>
        </form>
    </li>
</ul>
